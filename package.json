{
  "name": "mini-dashboard",
  "version": "0.1.4",
  "private": true,
  "dependencies": {
    "@meilisearch/instant-meilisearch": "^0.5.0",
    "@styled-system/should-forward-prop": "^5.1.5",
    "babel-loader": "8.1.0",
    "color": "^3.1.3",
    "eslint-plugin-import": "^2.24.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-instantsearch-dom": "^6.11.0",
    "react-json-view": "^1.21.3",
    "react-lazy-load-image-component": "^1.5.1",
    "react-scripts": "4.0.3",
    "reakit": "^1.3.8",
    "styled-components": "^5.3.1",
    "styled-system": "^5.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:ci": "REACT_APP_MEILI_SERVER_ADDRESS=http://meilisearch:7700 react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "cy:open": "cypress open --env env=watch",
    "cy:run:test-no-meilisearch": "cypress run --env env=watch --spec 'cypress/integration/test-no-meilisearch.js'",
    "cy:run": "cypress run --env env=watch --config ignoreTestFiles=**/*/test-no-meilisearch.js",
    "icons": "npx @svgr/cli --title-prop --no-dimensions --replace-attr-values \"#39486E=currentColor,#959DB3=currentColor\" -d src/components/icons src/components/icons/svg"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.3.1",
    "@storybook/addon-links": "^6.3.1",
    "@storybook/node-logger": "^6.2.8",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.1",
    "@svgr/cli": "^5.5.0",
    "cypress": "^7.6.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.1"
  }
}
