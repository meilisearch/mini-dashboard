{
  "name": "mini-dashboard",
  "version": "0.2.20",
  "private": true,
  "dependencies": {
    "@meilisearch/instant-meilisearch": "0.25",
    "@styled-system/should-forward-prop": "^5.1.5",
    "babel-loader": "9.1.3",
    "color": "^5.0.0",
    "eslint-plugin-import": "^2.29.1",
    "meilisearch": "0.48.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-instantsearch-dom": "^6.40.4",
    "react-json-view": "^1.21.3",
    "react-lazy-load-image-component": "^1.6.3",
    "reakit": "^1.3.11",
    "styled-components": "^5.3.9",
    "styled-system": "^5.1.5"
  },
  "scripts": {
    "version-script": "node version-script.js",
    "validate-env": "node validate-env.js",
    "prestart": "yarn version-script && yarn validate-env",
    "prebuild": "yarn version-script && yarn validate-env",
    "start": "vite",
    "start:ci": "REACT_APP_MEILI_SERVER_ADDRESS=http://meilisearch:7700 vite",
    "build": "vite build",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "prettier-check": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cy:open": "cypress open",
    "cy:run:test-no-meilisearch": "cypress run --spec '**/*/test-no-meilisearch.cy.js'",
    "cy:run:test-api-key-required": "cypress run --spec '**/*/test-api-key-required.cy.js,**/*/test-api-key-query-param.cy.js'",
    "cy:run": "cypress run --config excludeSpecPattern=['**/*/test-no-meilisearch.cy.js','**/*/test-api-key-required.cy.js','**/*/test-api-key-query-param.cy.js']",
    "icons": "npx @svgr/cli --title-prop --no-dimensions --replace-attr-values \"#39486E=currentColor,#959DB3=currentColor\" -d src/components/icons src/components/icons/svg",
    "dev": "vite"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^8.6.11",
    "@storybook/addon-essentials": "^8.6.11",
    "@storybook/addon-links": "^8.6.11",
    "@storybook/node-logger": "^8.6.11",
    "@storybook/react": "^8.6.11",
    "@storybook/react-vite": "^8.6.11",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@vitejs/plugin-react": "^4.6.0",
    "cypress": "^14.1.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.3.3",
    "storybook": "^8.6.11",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "packageManager": "yarn@4.6.0",
  "resolutions": {
    "css-select/nth-check": "^2.0.1",
    "css-minimizer-webpack-plugin/postcss": "^8.4.31",
    "react-scripts/postcss": "^8.4.31",
    "resolve-url-loader/postcss": "^8.4.31",
    "@storybook/core/esbuild": "^0.25.0"
  }
}
