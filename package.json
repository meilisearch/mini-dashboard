{
  "name": "mini-dashboard",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "@meilisearch/instant-meilisearch": "0.25",
    "@styled-system/should-forward-prop": "^5.1.5",
    "color": "^5.0.0",
    "dotenv": "^17.2.4",
    "eslint-plugin-import": "^2.29.1",
    "meilisearch": "0.48.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-instantsearch-dom": "^6.40.4",
    "react-json-view": "^1.21.3",
    "react-lazy-load-image-component": "^1.6.3",
    "reakit": "^1.3.11",
    "styled-components": "^5.3.9",
    "styled-system": "^5.1.5"
  },
  "scripts": {
    "version-script": "node version-script.js",
    "validate-env": "node validate-env.js",
    "prestart": "yarn version-script && yarn validate-env",
    "prebuild": "yarn version-script && yarn validate-env",
    "dev": "vite",
    "dev:ci": "VITE_MEILI_SERVER_ADDRESS=http://meilisearch:7700 vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "lint": "eslint .",
    "prettier-check": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cy:open": "cypress open",
    "cy:run:test-no-meilisearch": "cypress run --spec '**/*/test-no-meilisearch.cy.js'",
    "cy:run:test-api-key-required": "cypress run --spec '**/*/test-api-key-required.cy.js,**/*/test-api-key-query-param.cy.js'",
    "cy:run": "cypress run --config excludeSpecPattern=['**/*/test-no-meilisearch.cy.js','**/*/test-api-key-required.cy.js','**/*/test-api-key-query-param.cy.js']",
    "icons": "npx @svgr/cli --title-prop --no-dimensions --replace-attr-values \"#39486E=currentColor,#959DB3=currentColor\" -d src/components/icons src/components/icons/svg"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^8.6.15",
    "@storybook/addon-backgrounds": "^8.6.15",
    "@storybook/addon-controls": "^8.6.15",
    "@storybook/addon-docs": "^8.6.15",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/addon-measure": "^8.6.15",
    "@storybook/addon-outline": "^8.6.15",
    "@storybook/addon-toolbars": "^8.6.15",
    "@storybook/addon-viewport": "^8.6.15",
    "@storybook/blocks": "8.6.14",
    "@storybook/global": "^5.0.0",
    "@storybook/node-logger": "^8.4.7",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@svgr/cli": "^8.1.0",
    "@vitejs/plugin-react": "^4.4.1",
    "cypress": "^14.5.3",
    "dequal": "^2.0.3",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^28.0.0",
    "memoizerific": "^1.11.3",
    "polished": "^4.3.1",
    "prettier": "^3.3.3",
    "storybook": "^8.6.15",
    "ts-dedent": "^2.2.0",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^4.0.18"
  },
  "packageManager": "yarn@4.6.0",
  "resolutions": {
    "css-select/nth-check": "^2.0.1",
    "css-minimizer-webpack-plugin/postcss": "^8.4.31",
    "resolve-url-loader/postcss": "^8.4.31",
    "@storybook/core/esbuild": "^0.25.0",
    "js-yaml": "^4.1.1",
    "qs": "^6.14.1"
  }
}
